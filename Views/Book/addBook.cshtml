@model songProject.Models.Book
@{
    ViewData["Title"] = "Contact";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>
   
   @using (Html.BeginForm("addBook","Book",FormMethod.Post))
{

<div>
    Title:
    @Html.TextBoxFor(model => model.title)
    Author:
    @Html.TextBoxFor(model => model.author)
    type:
    @Html.TextBoxFor(model => model.type)
    published year:
    @Html.TextBoxFor(model => model.publishedYear)
    Summary:
    @Html.TextBoxFor(model => model.summary)
    Image:
    @Html.TextBoxFor(model => model.image, new {@class="div-control", @Value="http://lytherus.com/wp-content/uploads/2013/02/harry-potter-and-the-sorcerers-stone1.jpg"})
  


    <button type="submit" >add</button>
</div>

}